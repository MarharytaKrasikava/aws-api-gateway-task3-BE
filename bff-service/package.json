{
  "name": "marharytakrasikava-bff-service",
  "version": "1.0.0",
  "description": "Backend for Frontend express app",
  "scripts": {
    "build": "tsc",
    "start-dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "start": "npm install && tsc && node src/app.js",
    "eb-init": "eb init --platform node.js --region eu-west-1",
    "eb-create": "eb create development --single --cname marharytakrasikava-bff-api-dev"
  },
  "author": "HolyDaizy",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.3",
    "ts-node-dev": "^2.0.0"
  }
}
